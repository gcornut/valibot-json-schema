{
  "name": "@gcornut/valibot-json-schema",
  "version": "0.0.22",
  "packageManager": "yarn@3.6.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gcornut/valibot-json-schema.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "valibot",
    "schema",
    "cli",
    "json-schema"
  ],
  "type": "commonjs",
  "files": [
    "dist",
    "bin"
  ],
  "bin": "bin/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "check:types": "tsc --noEmit",
    "publish": "yarn build && npm publish",
    "run:cli": "esr cli",
    "test": "vitest",
    "type-check": "yarn tsc -p tsconfig.json",
    "lint": "yarn biome check .",
    "lint:ci": "yarn biome ci ."
  },
  "peerDependencies": {
    "@types/json-schema": ">= 7.0.14",
    "esbuild": ">= 0.18.20",
    "esbuild-runner": ">= 2.2.2",
    "valibot": ">= 0.21.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@types/json-schema": "^7.0.14",
    "@types/lodash": "^4.14.199",
    "@vitest/coverage-v8": "^0.34.6",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "commander": "^11.1.0",
    "dts-bundle-generator": "^8.1.1",
    "esbuild": "^0.18.20",
    "esbuild-runner": "^2.2.2",
    "lodash": "^4.17.21",
    "safe-stable-stringify": "^2.4.3",
    "tsup": "^7.2.0",
    "typescript": "5.1.3",
    "valibot": "^0.27.0",
    "vitest": "0.32.2"
  }
}
